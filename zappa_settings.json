{
    "stage": {
        "s3_bucket": "neo20iitkgp",
        "app_function": "dpr.application",
        "aws_region": "us-west-2",
        "use_apigateway": true,
        "memory_size": 512,
        "log_level": "DEBUG",
        "debug": true,
        "profile_name": "default",
        "environment_variables": {
            "FLASK_CONFIGURATION": "stage"
        },
        "events" : [
            {
                "function" : "s3_event_listener.write_to_rds_on_s3_metadata_put",
                "event_source": {
                    "arn": "arn:aws:s3:::neo20iitkgp",
                    "events": [
                        "s3:ObjectCreated:*"
                    ],
                    "key_filters" : [
                        {
                            "type" : "suffix",
                            "value": ".json"
                        },
                        {
                            "type" : "prefix",
                            "value": "metadata"
                        }
                    ]
                }
            }
        ]
    }
}